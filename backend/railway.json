{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "cd backend && java -jar target/delicias-da-katie-0.0.1-SNAPSHOT.jar --spring.profiles.active=prod",
    "healthcheckPath": "/actuator/health",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE"
  },
  "build.environment": {
    "JAVA_VERSION": "17",
    "MAVEN_VERSION": "3.9.5"
  }
} 